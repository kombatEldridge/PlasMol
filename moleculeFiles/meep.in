start source
    sourceType chirped
    sourceCenter -0.04
    sourceSize 0 0.1 0.1
    frequency 5
    width 0.1
    peakTime 5
    chirpRate -0.5
    isIntegrated True
end source

start simulation
    resolution 2000
    responseCutOff 1e-12
    cellLength 0.1
    pmlThickness 0.01
    totalTime 40 fs
    symmetries Y 1 Z -1
    surroundingMaterialIndex 1.33
    directionCalculation z
end simulation

start matplotlib
    output chirped10fslowres
    CSVlocation csv/
    IMGlocation img/
end matplotlib

start molecule
    moleculeCenter 0 0 0
    directionCalculation z
    quantumInputFile h2o.in
end molecule

